        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var Calendar = Packages.java.util.Calendar;
var Locale = Packages.java.util.Locale;
var SimpleDateFormat = Packages.java.text.SimpleDateFormat;

var spanish = new Locale("es", "ES");

var quarter = Math.floor((month-1) / 3) + 1;
var half = Math.floor((quarter-1) / 2) + 1;

var cal = Calendar.getInstance();
cal.set(year, month-1, day);
var d = cal.getTime()

var day_of_week_short_label_spa = new SimpleDateFormat("E",spanish).format(d);
var day_of_week_long_label_spa = new SimpleDateFormat("EEEEE",spanish).format(d);
var month_short_label_spa = new SimpleDateFormat("MMM",spanish).format(d);
var month_long_label_spa = new SimpleDateFormat("MMMMM",spanish).format(d);

var is_weekend = (idDiaSemana == Calendar.SUNDAY || idDiaSemana == Calendar.SATURDAY)?1:0;

var date = new Date(year, month-1, day);

var a0 = new Date(year - 1, 12-1, 22);
var a = new Date(year, 3-1, 22);
var b = new Date(year, 6-1, 22);
var c = new Date(year, 09-1, 22);
var d = new Date(year, 12-1, 22);
var d1 = new Date(year + 1, 3-1, 22);

var seasons = ['verano', 'otono', 'invierno', 'primavera'];

var season, seasonName;

if (a0 &lt;= date &amp;&amp; date &lt; a) {
    season = 1;
} else if (a &lt;= date &amp;&amp; date &lt; b) {
    season = 2;
} else if (b &lt;= date &amp;&amp; date &lt; c) {
    season = 3;
} else if (c &lt;= date &amp;&amp; date &lt; d) {
    season = 4;
} else if (d &lt;= date &amp;&amp; date &lt; d1) {
    season = 1;
}

seasonName = seasons[season-1];</jsScript_script>
